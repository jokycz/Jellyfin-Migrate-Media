<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- Make package sources deterministic for this repo (overrides user/machine-wide settings). -->
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="UuAppServer package source" value="https://repo.plus4u.net/repository/uu_appg01_serverkit-dotnet-alpha" />
  </packageSources>

  <!-- Required when Central Package Management is enabled and multiple package sources exist. -->
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <!-- Most packages should come from nuget.org -->
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>

